<odoo>
    <record id ="estate_property_action" model = "ir.actions.act_window">
        <field name ="name">RealEstate</field>
        <field name ="res_model">estate.property</field>
        <field name ="view_mode">tree,form</field>
        <field name="context"> {'search_default_filter_state_name': True} </field>
    </record>
    <record id ="check_action" model = "ir.actions.act_window">
        <field name ="name">User_Profile</field>
        <field name ="res_model">user.data</field>
        <field name ="view_mode">tree,form</field>
        <field name = "help" type = "html">
            <p class ="o_view_nocontent_smiling_face">
                add a new user
            </p>
        </field>
    </record>

    <record id ="property_type_action" model = "ir.actions.act_window">
        <field name ="name">Property_type</field>
        <field name ="res_model">estate.property.type</field>
        <field name ="view_mode">tree,form</field>
        <field name = "help" type = "html">
            <p class ="o_view_nocontent_smiling_face">
                add a new user
            </p>
        </field>
    </record>

     <record id ="property_tag_action" model = "ir.actions.act_window">
        <field name ="name">Property Tags</field>
        <field name ="res_model">estate.property.tags</field>
        <field name ="view_mode">tree,form</field>
        <field name = "help" type = "html">
            <p class ="o_view_nocontent_smiling_face">
                add a new user
            </p>
        </field>
    </record>

    <record id ="transactions_action" model = "ir.actions.act_window">
        <field name ="name">Transactions</field>
        <field name ="res_model">estate.transactions</field>
        <field name ="view_mode">tree,form</field>
        <field name = "help" type = "html">
            <p class ="o_view_nocontent_smiling_face">
                add a new user
            </p>
        </field>
    </record>

</odoo>